		}
		OBJECT suma_ASIENTO_TIPOASIENTO {
			value = "=input.ASIENTO_TIPOASIENTO";
			halignment = "right";
			posx = 102;
			sizex = 1;
			aggregate = sum;
		}

	}

	pageorientation = landscape;
}
