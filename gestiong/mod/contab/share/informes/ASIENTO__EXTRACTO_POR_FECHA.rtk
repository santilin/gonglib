	}
	REPORTFOOTER page_footer {
		sizey = 1.4;
		bordertopstyle = solid;
		paddingtop = 0.4;
		style = pagefooter;


		OBJECT suma_APUNTE_NUMAPUNTE {
			value = "=input.APUNTE_NUMAPUNTE";
			halignment = "right";
			posx = 46;
			sizex = 9;
			aggregate = sum;
		}
		OBJECT suma_APUNTE_DEBE {
			value = "=input.APUNTE_DEBE";
			halignment = "right";
			posx = 74;
			sizex = 13;
			aggregate = sum;
		}
		OBJECT suma_APUNTE_HABER {
			value = "=input.APUNTE_HABER";
