			sizex = 17;
		}
		OBJECT APUNTE_DEBE {
			value = "=input.APUNTE_DEBE";
			halignment = "right";
			posx = 74;
			sizex = 13;
		}
		OBJECT APUNTE_HABER {
			value = "=input.APUNTE_HABER";
			halignment = "right";
